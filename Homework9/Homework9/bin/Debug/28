            function mySayGood (e) {
            	var x = e.clientX;
            	var y = e.clientY;
                var countObj = jq('#myZanCountsSpan');
                jq.ajax({
                    url:'apkapi/quotation.php',
                    type:"post",
                    dataType:"json",
                    data:{op:'saygood'},
                    success:function (msg) {
                        if (msg.rs==='success'){
                            countObj.html(parseInt(countObj.html())+1);
                        }else{
                        	alerts.show(msg.info,x,y);
                            alerts.hide();
                        }
                    }
                });
            }
            
