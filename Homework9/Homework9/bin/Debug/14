function existmobile(){
var mobile = true;
jQuery.ajax({
type:'get',
url:'api/bindmobile.php',
async: false,
dataType:'json',
success:function(data){
if(data.ret_no === 1){
jQuery(".bind-phonenum-pannel").css('display','block');
mobile = false;
}else if(data.ret_no === 2){
jQuery("#qiku_bindmobile").val(data.ret_data);
jQuery(".bind-phonenum-pannel").css('display','block');
mobile = false;
}
}
})
return mobile;
}
</script>
