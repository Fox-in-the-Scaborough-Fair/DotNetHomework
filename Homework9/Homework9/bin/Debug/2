            jq(document).on('click','.wait-praise-a',function(){
                if(!existmobile()){
                    return false;
                }
                var _this = jq(this);
                _this.removeClass('wait-praise-a');
                var data = {
                    formhash : jq("[name='formhash']").eq(-1).val(),
                    tid : _this.data('tid'),
                    pid : _this.data('pid'),
                    status : 1 
                }
                handdlePraise(data,1,_this);
            });
            jq(document).on('click','.praise-over-a',function(){
                if(!existmobile()){
