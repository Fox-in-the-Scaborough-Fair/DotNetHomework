            if (navigator.userAgent.indexOf('MSIE')>0) {
                document.getElementById("scbar").attachEvent("onclick", function(e) {
                    if (e.srcElement.className == "fold") {
                        e.srcElement.className = "";
                        e.srcElement.detachEvent('onclick');
                        return false;
                    }
                })
